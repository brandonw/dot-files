"pamixer --toggle-mute"
    m:0x10 + c:121
    Mod2 + XF86AudioMute

"pamixer --allow-boost --increase 5"
    m:0x10 + c:123
    Mod2 + XF86AudioRaiseVolume

"pamixer --allow-boost --decrease 5"
    m:0x10 + c:122
    Mod2 + XF86AudioLowerVolume

"xbacklight -10"
    m:0x10 + c:232
    Mod2 + XF86MonBrightnessDown

"xbacklight +10"
    m:0x10 + c:233
    Mod2 + XF86MonBrightnessUp
